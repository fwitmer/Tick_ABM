<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" defaultValue="__NULL__" />
	
	<parameter name="tick_count" displayName="Tick Count" type="int" 
		isReadOnly="false"
		defaultValue="10" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" 
	/>
	
	<parameter name="large_host_density" displayName="Large Host Density (per sq. kilometer)" type="double"
		isReadOnly="false"
		defaultValue="0.1"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
	/>
	
	
	<parameter name="tick_lifestage" displayName="Tick Life Stage" type="string"
		isReadOnly="false"
		defaultValue="egg"
	/>
	
	<parameter name="habitat_suitability" displayName="Tick Habitat Suitability Raster" type="double"
		isReadOnly="false"
		defaultValue="-1"
	/>

</parameters>
